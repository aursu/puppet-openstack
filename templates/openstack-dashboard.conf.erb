WSGIDaemonProcess dashboard<% if @wsgi_daemon_process_options.is_a?(Hash) %> <%= @wsgi_daemon_process_options.map { |o| o.join('=') }.join(' ') %><% end %>
WSGIProcessGroup dashboard
<%- if @wsgi_socket_prefix -%>
WSGISocketPrefix <%= @wsgi_socket_prefix %>
<%- end -%>
<%- if @wsgi_application_group -%>
WSGIApplicationGroup <%= @wsgi_application_group %>
<%- end -%>

WSGIScriptAlias /dashboard <%= @wsgi_script %><% if @wsgi_script_options.is_a?(Hash) %> <%= @wsgi_script_options.map { |o| o.join('=') }.join(' ') %><% end %>
Alias /static <%= @static_content_path %>
Alias /dashboard/static <%= @static_content_path %>

<Directory <%= @wsgi_script_path %>>
  Options All
  AllowOverride All
  Require all granted
</Directory>

<Directory <%= @static_content_path %>>
  Options All
  AllowOverride All
  Require all granted
</Directory>
